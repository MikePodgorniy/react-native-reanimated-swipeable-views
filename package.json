{
  "name": "react-native-reanimated-swipeable-list",
  "version": "0.0.1",
  "description": "Natively animated swipeable FlatList, SectionList & HOC for wrapping custom lists",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "preversion": "npm run build && npm run lint",
    "postversion": "git push && git push --tags",
    "build": "rm -rf dist && mkdir dist && cp src/index.d.ts dist/index.d.ts && tsc -p src",
    "lint": "eslint . --ext .ts,.tsx",
    "watch": "npm run build -- --watch"
  },
  "keywords": [
    "react-native",
    "swipeable-list"
  ],
  "files": [
    "example",
    "dist",
    "src"
  ],
  "author": "iyegoroff <iegoroff@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/iyegoroff/react-native-reanimated-swipeable-list/issues"
  },
  "homepage": "https://github.com/iyegoroff/react-native-reanimated-swipeable-list#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iyegoroff/react-native-reanimated-swipeable-list.git"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*",
    "react-native-reanimated": "*",
    "react-native-gesture-handler": "*"
  },
  "devDependencies": {
    "@types/react": "^16.9.32",
    "@types/react-native": "^0.62.1",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "@react-native-community/eslint-config": "0.0.5",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-config-standard-with-typescript": "^15.0.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-no-null": "^1.0.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "eslint-plugin-standard": "^4.0.1",
    "prettier": "^2.0.2",
    "react-native-gesture-handler": "^1.6.1",
    "react-native-reanimated": "^1.8.0",
    "typescript": "^3.8.3"
  }
}
